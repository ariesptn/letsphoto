<%- include('./partials/header')%>
<img src="/uploads/<%=imageData.filename%>">
<form action='/comment/<%=imageCommentData.filename%>' method="POST">
    <input type="text" name="comment">
    <input type="submit" value="Post">
</form>
<div>
    <% imageCommentData.forEach(imageComment=>{ %>
        <div><%=imageComment.User %></div>
        <div><%=imageComment.Comment.comment %></div>
    <% }) %>
</div>
<%- include('./partials/footer')%>